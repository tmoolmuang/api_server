<div>
  <%= link_to("<< Back to List", { :action => "index" }) %>
  <div>
    <h3>Create New Alien</h3>
    <%= form_for(:alien, :url => { :action => "create" }) do |f| %>
      <table>
        <tr>
          <th>Name</th>
          <td><%= f.text_field(:name) %></td>
        </tr>
        <tr>
          <th>Year</th>
          <td><%= f.text_field(:year) %></td>
        </tr>
        <tr>
          <th>Month</th>
          <td><%= f.text_field(:month) %></td>
        </tr>
        <tr>
          <th>Origin</th>
          <td><%= f.text_field(:origin) %></td>
        </tr>
      </table>
      <div>
        <%= submit_tag("Create Alien") %>
      </div>
    <% end %>
  </div>
</div>